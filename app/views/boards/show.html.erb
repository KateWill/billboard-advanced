<h1><%= @board.title %></h1>



<%= link_to 'All Songs', board_songs_path(@board) %>
<%= link_to 'Add new song', new_board_song_path(@board) %>
<ol>

<% @ranks.each do |song| %>

  <li><strong>Song Title:</strong> <%= link_to song.name, board_song_path(@board, song) %> | <strong>Rank:</strong> <%= song.rank %></li>
 
<% end %>
</ol>
